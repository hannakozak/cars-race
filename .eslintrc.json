{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "import/prefer-default-export": 0,
    "no-trenary": 0,
    "quotes": ["error", "double"],
    "comma-dangle": "off"
  }
}
